<div class="page-header">
  <h1>
    User ids
    <a href="/user_ids/new" class="btn btn-lg btn-success">New User</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Reviews</th>
        <th>Email</th>
        <th>Password</th>
        <th>Actions</th>
      </tr>

      <% @user_ids.each do |user_id| %>
      <tr>
        <td><%= user_id.reviews_id %></td>
        <td><%= user_id.email %></td>
        <td><%= user_id.password %></td>
        <td>
          <a href="/user_ids/<%= user_id.id %>" class="btn btn-primary">Show</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
